    <servlet-name>show</servlet-name>
    <url-pattern>/show</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>specialty</servlet-name>
    <servlet-class>uwazima.Specialty</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>specialty</servlet-name>
    <url-pattern>/specialty</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>place</servlet-name>
    <servlet-class>uwazima.Place</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>place</servlet-name>
    <url-pattern>/place</url-pattern>
  </servlet-mapping>
</web-app>