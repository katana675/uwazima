<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>uwazima</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>top</servlet-name>
    <servlet-class>uwazima.Top</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>top</servlet-name>
    <url-pattern>/top</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>gaiya</servlet-name>
    <servlet-class>uwazima.Gaiya</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>gaiya</servlet-name>
    <url-pattern>/gaiya</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>show</servlet-name>
    <servlet-class>uwazima.Show</servlet-class>
  </servlet>
